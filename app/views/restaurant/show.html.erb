<div class="row">
  <div class= "nine columns">
    <h2> <%= @restaurant.name %></h2>
    <h3> <small> located at <%= @restaurant.address %> </small> </h3>
    <div id='chart'></div>
    <%= render_chart(@chart, 'chart') %>
  </div>
  <div class= "three columns ">
    <h5>Recent audits</h5>
    <%= render :partial=>"audits_table", :locals =>{:audits => audits_by_date( @restaurant ) } %>
  </div>
</div>


<table id="health_scores">
  <thead>
    <tr>
      <th><%= sortable( "name" ) %></th>
      <th>Address</th>
      <th><%= sortable("score" ) %></th>
      <th><%= sortable( "date" )%></th>
    </tr>
  </thead>
  <tbody>
  <% audits.each do |audit| %>
    <tr>
      <td> <%=audit.name %> </td>
      <td> <%=audit.address %> </td>
      <td> <%=audit.score %> </td>
      <td> <%=audit.date.to_formatted_s(:display) %> </td>
    </tr>
  <% end %>
  </tbody>
</table>
